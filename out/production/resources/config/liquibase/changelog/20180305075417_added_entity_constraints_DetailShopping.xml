<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.5.xsd">
    <!--
        Added the constraints for entity DetailShopping.
    -->
    <changeSet id="20180305075417-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="shopping_id"
                                 baseTableName="detail_shopping"
                                 constraintName="fk_detail_shopping_shopping_id"
                                 referencedColumnNames="id"
                                 referencedTableName="shopping"/>

        <addForeignKeyConstraint baseColumnNames="clothes_id"
                                 baseTableName="detail_shopping"
                                 constraintName="fk_detail_shopping_clothes_id"
                                 referencedColumnNames="id"
                                 referencedTableName="clothes"/>

    </changeSet>
</databaseChangeLog>
